    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-jessie": "0.0.3",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "esm": "^3.2.5",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2",
    "tap": "^14.10.2",
    "tap-spec": "^5.0.0",
